                SU.Captcha._callbacks.push(callback);
                SU.Modal.loadURL("/captcha", {iframe:true}).open();
            },

            executeCallbacks: function() {
                for (var i=0, callback; callback=SU.Captcha._callbacks[i]; i++) {
