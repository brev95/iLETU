<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iletu"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="7" />
        <!--  android:targetSdkVersion="17" /> -->
    
    <uses-permission android:name = "android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/iletu_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:logo="@drawable/iletu_icon">
        <activity
            android:name="com.example.iletu.MainActivity"
            android:label="@string/app_name" 
            android:configChanges="orientation|screenSize" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.example.iletu.DetailActivity"
            android:label="@string/app_name" >
        </activity>
        <activity 
            android:name="com.example.iletu.WebViewActivity" 
            android:label="@string/app_name">
        </activity>
        <activity 
            android:name="com.example.iletu.IntramuralActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="DetailWebActivity" android:label="@string/app_name"></activity>
    </application>
</manifest>